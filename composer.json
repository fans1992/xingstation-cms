{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-cors": "^0.11.0",
        "barryvdh/laravel-ide-helper": "^2.4",
        "baum/baum": "^1.1",
        "dingo/api": "2.0.0-alpha1",
        "doctrine/dbal": "^2.7",
        "fideloper/proxy": "^4.0",
        "gregwar/captcha": "^1.1",
        "hashids/hashids": "1.0.0",
        "intervention/image": "^2.4",
        "jenssegers/agent": "^2.6",
        "jenssegers/mongodb": "^3.4",
        "laravel/framework": "5.6.*",
        "laravel/horizon": "^1.2",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "liyu/dingo-serializer-switch": "^0.3.0",
        "maatwebsite/excel": "^3.0",
        "mews/purifier": "~2.0",
        "overtrue/easy-sms": "^1.0",
        "overtrue/laravel-wechat": "~4.0",
        "phpoffice/phpspreadsheet": "^1.2",
        "predis/predis": "^1.1",
        "socialiteproviders/manager": "^3.3",
        "spatie/laravel-activitylog": "^2.5",
        "spatie/laravel-permission": "~2.7",
        "tymon/jwt-auth": "1.0.0-rc.2",
        "yansongda/pay": "^2.4",
        "zgldh/qiniu-laravel-storage": "^0.9.2"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mnabialek/laravel-sql-logger": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "xethron/migrations-generator": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.laravel-china.org"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
